    <dct:title xml:lang="es" >Mujeres Jóvenes Rurales en Colombia</dct:title>
    <dc:author>Daniela Buendía Silva</dc:author>
    <dct:date rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2013-10-11</dct:date>
