    <dct:title xml:lang="es" >Disputas en la gestión de la identidad y el ejercicio de los gobiernos indígenas en Bolivia</dct:title>
    <dc:author>Pedro Pachaguaya y Samuel Flores</dc:author>
    <dct:date rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2016-12-29</dct:date>
    <dct:extent>17</dct:extent>
