    <rdf:type rdf:resource="http://purl.org/ontology/bibo/Document" />
    <dct:title xml:lang="pt" >Terras Indígenas: garantía de sobrevivência física e cultural dos povos</dct:title>
    <dc:author>Diego do Santos</dc:author>
    <dct:date rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2012-10-11</dct:date>
    <dct:extent>24</dct:extent>
