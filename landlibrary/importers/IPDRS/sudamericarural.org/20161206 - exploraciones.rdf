	<rdf:Description rdf:about="http://www.sudamericarural.org/index.php/nuestra-produccion/exploraciones/exploracion/21">
		<dct:title xml:lang="es" >Jóvenes rurales actores y autores</dct:title>
		<dc:author>Matias Pontoriero</dc:author>
		<dct:date rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2014-02-10</dct:date>
