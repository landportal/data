	<rdf:Description rdf:about="http://www.sudamericarural.org/nuestra-produccion/multimedia/multimedia/4">
		<dct:title xml:lang="es" >Movimiento regional por la tierra: entrevista a Clementina Barajas</dct:title>
		<dc:author>IPDRS</dc:author>
		<dct:identifier>IPDRS-Videos-4</dct:identifier>
		<dct:extent>344</dct:extent>
