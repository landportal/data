  <rdf:Description rdf:about="http://www.suelourbano.org/bibliotecas/2017/09/29/a-regulacao-institucional-da-propriedade-da-terra-no-brasil-uma-necessidade-urgente/">
    <rdf:type rdf:resource="http://purl.org/ontology/bibo/Document" />
