  <rdf:Description rdf:about="http://data.landportal.info/voc/landvoc/theme/T-CONFLICT">
    <skos:related rdf:resource="http://aims.fao.org/aos/agrovoc/c_6f79699c"/>
    <rdf:type rdf:resource="http://www.w3.org/2004/02/skos/core#Concept"/>
    <skos:notation>T-CONFLICT</skos:notation>
    <skos:related rdf:resource="http://aims.fao.org/aos/agrovoc/c_ee93bbbe"/>
    <schema:image rdf:resource="https://landportal.org/sites/landportal.org/files/icons-landportal-05-transparent.png"/>
    <rdfs:seeAlso rdf:resource="https://landportal.org/voc/themes/land-conflicts"/>
