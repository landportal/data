  <rdf:Description rdf:about="http://data.landportal.info/dataset/NKT-VGGT16/observation/NKT-VGGT16-1E_IND_2016">
    <cex:ref-indicator rdf:resource="http://data.landportal.info/indicator/NKT-VGGT16-1E"/>
    <cex:ref-area rdf:resource="http://data.landportal.info/geo/IND"/>
