    <qb:dataSet rdf:resource="http://data.landportal.info/dataset/NKT-VGGT16"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.landportal.info/dataset/NKT-VGGT16/observation/NKT-VGGT16-1E_TWN_2016">
