    <cex:ref-area rdf:resource="http://data.landportal.info/geo/BFA"/>
    <qb:dataSet rdf:resource="http://data.landportal.info/dataset/NKT-VGGT16"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.landportal.info/dataset/NKT-VGGT16/observation/NKT-VGGT16-1E_ZWE_2016">
    <cex:ref-time rdf:resource="http://data.landportal.info/time/2016"/>
    <rdfs:label xml:lang="en">Observation of ZWE within the period of 2016 for indicator NKT-VGGT16-1E</rdfs:label>
    <rdf:type rdf:resource="http://purl.org/linked-data/cube#Observation"/>
    <cex:ref-indicator rdf:resource="http://data.landportal.info/indicator/NKT-VGGT16-1E"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Section 20 of the Land Acquisition Act (LAA), 2002 provides compensation based on the value of the loss of land for land other than agricultural land required for resettlement purposes. When agricultural land is acquired for resettlement purposes, no compensation is payable in respect of its acquisition, except for improvements made on it before its acquisition (Art. 72(3)(a); Art. 295(1) of Zimbabwe's Constitution, 2013). However, when non-agricultural land is acquired for non-resettlement purposes, compensation is for "any loss reasonable incurred" (Sec. 20, LAA 2002)..." </rdfs:comment>
    <qb:dataSet rdf:resource="http://data.landportal.info/dataset/NKT-VGGT16"/>
    <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-03-13</dct:issued>
