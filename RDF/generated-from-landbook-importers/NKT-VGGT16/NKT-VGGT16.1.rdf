    <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-03-13</dct:issued>
    <rdfs:label xml:lang="en">Observation of GHA within the period of 2016 for indicator NKT-VGGT16-1E</rdfs:label>
