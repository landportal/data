  <rdf:Description rdf:about="http://data.landportal.info/dataset/NKT-VGGT16/observation/NKT-VGGT16-3A_LSO_2017">
    <cex:ref-indicator rdf:resource="http://data.landportal.info/indicator/NKT-VGGT16-3A"/>
