  <rdf:Description rdf:about="http://data.landportal.info/dataset/NKT-VGGT16/observation/NKT-VGGT16-3E_HND_2017">
    <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-09-08</dct:issued>
