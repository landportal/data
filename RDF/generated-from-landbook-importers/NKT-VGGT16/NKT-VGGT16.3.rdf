  <rdf:Description rdf:about="http://data.landportal.info/dataset/NKT-VGGT16/observation/NKT-VGGT16-3A_AGO_2017">
    <rdfs:label xml:lang="en">Observation of AGO within the period of 2017 for indicator NKT-VGGT16-3A</rdfs:label>
