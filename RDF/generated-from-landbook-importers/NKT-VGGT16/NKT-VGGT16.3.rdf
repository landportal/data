    <cex:value rdf:datatype="http://www.w3.org/2001/XMLSchema#string">A</cex:value>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.landportal.info/dataset/NKT-VGGT16/observation/NKT-VGGT16-3G_IND_2017">
