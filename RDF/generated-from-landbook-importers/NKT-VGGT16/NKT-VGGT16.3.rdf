    <qb:dataSet rdf:resource="http://data.landportal.info/dataset/NKT-VGGT16"/>
    <cex:computation rdf:resource="http://purl.org/weso/ontology/computex#Raw"/>
    <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-09-08</dct:issued>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.landportal.info/dataset/NKT-VGGT16/observation/NKT-VGGT16-3A_HND_2017">
    <cex:ref-time rdf:resource="http://data.landportal.info/time/2017"/>
    <cex:computation rdf:resource="http://purl.org/weso/ontology/computex#Raw"/>
    <rdfs:label xml:lang="en">Observation of HND within the period of 2017 for indicator NKT-VGGT16-3A</rdfs:label>
    <cex:value rdf:datatype="http://www.w3.org/2001/XMLSchema#string">C</cex:value>
    <qb:dataSet rdf:resource="http://data.landportal.info/dataset/NKT-VGGT16"/>
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Article 82(2) of Decree No. 82-2004 (Property Law) provides that the expert valuers must make an appraisal made by experts based cadastral criteria, financial valuation, technical and market. The laws assessed do not establish alternatives to the fair market value approach to calculating compensation in cases where land markets are weak or non-existent.</rdfs:comment>
    <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-09-08</dct:issued>
    <cex:ref-area rdf:resource="http://data.landportal.info/geo/HND"/>
    <cex:ref-indicator rdf:resource="http://data.landportal.info/indicator/NKT-VGGT16-3A"/>
    <rdf:type rdf:resource="http://purl.org/linked-data/cube#Observation"/>
    <sdmx-concept:obsStatus rdf:resource="http://purl.org/linked-data/sdmx/2009/code#obsStatus-A"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.landportal.info/dataset/NKT-VGGT16/observation/NKT-VGGT16-3E_AFG_2017">
    <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">Article 14 of the Law on Land Expropriation, 2000 (amended 2005) provides that "Instead of paying the price for an expropriated land to the owner of such land, another piece of land, equivalent to such land, may be distributed to the owner upon his agreement. If such new land has a grade higher than that expropriated, the owner shall pay the difference to the Emirates and vice versa."</rdfs:comment>
    <cex:ref-area rdf:resource="http://data.landportal.info/geo/AFG"/>
    <cex:computation rdf:resource="http://purl.org/weso/ontology/computex#Raw"/>
    <cex:ref-indicator rdf:resource="http://data.landportal.info/indicator/NKT-VGGT16-3E"/>
    <rdf:type rdf:resource="http://purl.org/linked-data/cube#Observation"/>
    <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-09-08</dct:issued>
    <qb:dataSet rdf:resource="http://data.landportal.info/dataset/NKT-VGGT16"/>
    <rdfs:label xml:lang="en">Observation of AFG within the period of 2017 for indicator NKT-VGGT16-3E</rdfs:label>
    <cex:value rdf:datatype="http://www.w3.org/2001/XMLSchema#string">B</cex:value>
    <sdmx-concept:obsStatus rdf:resource="http://purl.org/linked-data/sdmx/2009/code#obsStatus-A"/>
    <cex:ref-time rdf:resource="http://data.landportal.info/time/2017"/>
  </rdf:Description>
  <rdf:Description rdf:about="http://data.landportal.info/dataset/NKT-VGGT16/observation/NKT-VGGT16-3H_BWA_2017">
    <rdf:type rdf:resource="http://purl.org/linked-data/cube#Observation"/>
    <qb:dataSet rdf:resource="http://data.landportal.info/dataset/NKT-VGGT16"/>
    <cex:ref-indicator rdf:resource="http://data.landportal.info/indicator/NKT-VGGT16-3H"/>
    <cex:ref-area rdf:resource="http://data.landportal.info/geo/BWA"/>
    <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2017-09-08</dct:issued>
    <cex:computation rdf:resource="http://purl.org/weso/ontology/computex#Raw"/>
