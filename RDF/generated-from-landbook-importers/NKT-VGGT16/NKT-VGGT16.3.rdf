  <rdf:Description rdf:about="http://data.landportal.info/dataset/NKT-VGGT16/observation/NKT-VGGT16-3E_MYS_2017">
    <rdfs:label xml:lang="en">Observation of MYS within the period of 2017 for indicator NKT-VGGT16-3E</rdfs:label>
