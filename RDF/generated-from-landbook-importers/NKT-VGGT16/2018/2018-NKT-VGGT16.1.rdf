  <rdf:Description rdf:about="http://data.landportal.info/dataset/NKT-VGGT16/observation/NKT-VGGT16-1A_ECU_2018">
    <rdfs:label xml:lang="en">Observation of ECU within the period of 2018 for indicator NKT-VGGT16-1A</rdfs:label>
    <cex:ref-time rdf:resource="http://data.landportal.info/time/2018"/>
    <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-06-18</dct:issued>
