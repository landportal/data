  <rdf:Description rdf:about="http://data.landportal.info/dataset/LMM-PICL/observation/LMM-PICL-3NFR_NER_2017">
    <dct:issued rdf:datatype="http://www.w3.org/2001/XMLSchema#date">2018-12-17</dct:issued>
