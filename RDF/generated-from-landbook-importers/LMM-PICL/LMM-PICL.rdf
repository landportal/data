  <rdf:Description rdf:about="http://data.landportal.info/dataset/LMM-PICL/observation/LMM-PICL-2FR_CAF_2017">
    <cex:ref-time rdf:resource="http://data.landportal.info/time/2017"/>
    <sdmx-concept:obsStatus rdf:resource="http://purl.org/linked-data/sdmx/2009/code#obsStatus-A"/>
