  <rdf:Description rdf:about="http://data.landportal.info/dataset/LMM-PICL/observation/LMM-PICL-1TOT_NGA_2017">
    <cex:ref-area rdf:resource="http://data.landportal.info/geo/NGA"/>
    <cex:ref-indicator rdf:resource="http://data.landportal.info/indicator/LMM-PICL-1TOT"/>
