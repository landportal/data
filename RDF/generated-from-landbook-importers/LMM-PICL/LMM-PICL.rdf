  <rdf:Description rdf:about="http://data.landportal.info/dataset/LMM-PICL/observation/LMM-PICL-1TOT_MOZ_2017">
    <cex:ref-area rdf:resource="http://data.landportal.info/geo/MOZ"/>
    <cex:ref-indicator rdf:resource="http://data.landportal.info/indicator/LMM-PICL-1TOT"/>
    <cex:ref-time rdf:resource="http://data.landportal.info/time/2017"/>
