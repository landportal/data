  <rdf:Description rdf:about="http://data.landportal.info/dataset/LMM-PICL/observation/LMM-PICL-3NFR_MLI_2017">
    <cex:ref-indicator rdf:resource="http://data.landportal.info/indicator/LMM-PICL-3NFR"/>
    <qb:dataSet rdf:resource="http://data.landportal.info/dataset/LMM-PICL"/>
