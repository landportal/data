  <rdf:Description rdf:about="http://data.landportal.info/dataset/LMM-PICL/observation/LMM-PICL-2FR_ZAF_2017">
    <sdmx-concept:obsStatus rdf:resource="http://purl.org/linked-data/sdmx/2009/code#obsStatus-A"/>
    <cex:ref-area rdf:resource="http://data.landportal.info/geo/ZAF"/>
