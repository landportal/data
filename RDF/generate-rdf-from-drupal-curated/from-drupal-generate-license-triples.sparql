PREFIX skos: <http://www.w3.org/2004/02/skos/core#>
PREFIX rdfs: <http://www.w3.org/2000/01/rdf-schema#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>
PREFIX odrl: <http://www.w3.org/ns/odrl/2/>
PREFIX dct: <http://purl.org/dc/terms/>

CONSTRUCT {
   ?licenseURI a odrl:Policy ;
                rdfs:label ?futureLabel ;
                dct:identifier ?acronym ;
#				owl:sameAs ?rdflicenseURI ;
                rdfs:seeAlso ?licenseDrupalCurated.
}
FROM <http://licenses.landportal.info>
WHERE {
?licenseDrupal a odrl:Policy ;
                rdfs:label ?futureLabel ;
                dct:identifier ?acronym .
				OPTIONAL {?licenseDrupal owl:sameAs ?rdflicenseURI. }
BIND (IRI(REPLACE(REPLACE(STR(?licenseDrupal), "https://","http://"),"beta.landportal.info","landportal.info")) AS ?licenseDrupalCurated)
BIND (IRI(CONCAT("http://data.landportal.info/license/",?acronym)) AS ?licenseURI)
}
