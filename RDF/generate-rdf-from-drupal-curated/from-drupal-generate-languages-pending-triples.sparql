# Generate triple to complement the ones generated by Drupal

PREFIX lvont: <http://lexvo.org/ontology#>
PREFIX owl: <http://www.w3.org/2002/07/owl#>

CONSTRUCT {
   ?languageURI owl:sameAs ?lexvoURI ;
}
FROM <http://languages.landportal.info>
WHERE {
?languageDrupal a lvont:Language ;
                lvont:iso639P3Code ?iso639P3Code .
BIND (IRI(CONCAT("http://lexvo.org/id/iso639-3/",?iso639P3Code)) AS ?lexvoURI)
}
